{
  "name": "shadowsocks-electron",
  "version": "1.2.4",
  "description": "Shadowsocks GUI client with cross-platform desktop support.",
  "author": {
    "name": "nojsja",
    "email": "yangwei13@outlook.com"
  },
  "repository": "https://github.com/nojsja/shadowsocks-electron",
  "license": "GPL-3.0",
  "homepage": ".",
  "main": "public/electron.js",
  "private": true,
  "scripts": {
    "start:renderer": "cross-env NODE_ENV=development cross-env PORT=3001 cross-env BROWSER=none webpack serve",
    "start:main": "nodemon --config nodemon.json",
    "start:main:inspect": "npm run build:main && cross-env electron . --inspect=5222",
    "start": "concurrently \"npm run start:renderer\" \"wait-on http://localhost:3001 && npm run start:main\"",
    "build:renderer": "webpack --config webpack.prod.config.js --progress",
    "build:main": "npx tsc -p main/tsconfig.json",
    "build": "npm run build:main && npm run build:renderer",
    "pack:linux": "electron-builder --linux",
    "pack:mac": "electron-builder --mac",
    "pack:win": "electron-builder --win",
    "publish:linux": "electron-builder --linux --p always",
    "publish:mac": "electron-builder --mac --p always",
    "publish:win": "electron-builder --win --p always",
    "publish:linux:win": "electron-builder --win --linux --p always",
    "publish:snap": "snapcraft login && snapcraft upload dist/shadowsocks-electron_1.1.2_amd64.snap",
    "prebuild:renderer": "rimraf build/",
    "prebuild:main": "rimraf public/",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "clean": "rm -rf dist/linux-*unpacked && rm -rf dist/__snap* && rm -rf dist/win-*unpacked",
    "lint": "eslint --ext .tsx,.ts .",
    "typecheck": "tsc --noUnusedLocals",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@material-ui/pickers": "^3.3.10",
    "@sentry/electron": "^4.2.0",
    "@types/i18n": "^0.13.2",
    "@types/lodash": "^4.14.191",
    "chalk": "4.x",
    "classnames": "^2.3.2",
    "clsx": "^1.1.1",
    "crawler": "^1.4.0",
    "electron-is-dev": "1.1.0",
    "electron-re": "^1.2.7",
    "electron-store": "^8.0.1",
    "electron-updater": "^5.3.0",
    "electron-window-state": "^5.0.3",
    "fs-extra": "9.0.0",
    "i18n": "^0.14.2",
    "i18next": "^20.6.1",
    "ipv6": "*",
    "js-base64": "^2.4.0",
    "jsqr": "^1.4.0",
    "lodash": "^4.17.21",
    "node-abort-controller": "^3.1.1",
    "node-fetch": "2.6.0",
    "node-schedule": "^2.1.1",
    "prop-types": "^15.8.1",
    "puppeteer": "^19.5.2",
    "qrcode": "^1.4.4",
    "react-hook-form": "^7.42.0",
    "react-i18next": "^11.12.0",
    "socks": "1.x",
    "tapable": "^2.2.1",
    "v8-compile-cache": "^2.3.0",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "^4.5.5"
  },
