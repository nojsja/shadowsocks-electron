{
  "name": "shadowsocks-electron",
  "version": "1.2.4",
  "description": "Shadowsocks(R) GUI client with cross-platform desktop support.",
  "author": {
    "name": "nojsja",
    "email": "yangwei13@outlook.com"
  },
  "repository": "https://github.com/nojsja/shadowsocks-electron",
  "license": "GPL-3.0",
  "homepage": ".",
  "main": "public/electron.js",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "bootstrap": "cd ./src/renderer && npm install --force && cd .. && npm install",
    "start:renderer": "cd ./src/renderer && npm start && cd ..",
    "start:main": "nodemon --config nodemon.json",
    "start:main:cold": "electron .",
    "start:main:inspect": "npm run build:main && cross-env electron . --inspect=5222",
    "start": "turbo start:renderer start:main",
    "build:renderer": "cd ./src/renderer && npm run build && cd ..",
    "build:main": "rollup --config rollup.config.js",
    "build:main:tsc": "npx tsc -p ./src/main/tsconfig.json",
    "build": "turbo build:main build:renderer",
    "pack:linux": "electron-builder --linux",
    "pack:mac": "electron-builder --mac",
    "pack:win": "electron-builder --win",
    "publish:linux": "electron-builder --linux --p always",
    "publish:mac": "electron-builder --mac --p always",
    "publish:win": "electron-builder --win --p always",
    "prestart:main": "npm run lint && npm run build:main",
    "prebuild:renderer": "rimraf ./public/renderer/",
    "prebuild:main": "rimraf ./public/main/",
    "prepack:mac": "npm run build",
    "prepack:win": "npm run build",
    "prepack:linux": "npm run build",
    "prepublish:linux": "npm run build",
    "prepublish:mac": "npm run build",
    "prepublish:win": "npm run build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "clean": "rm -rf dist/linux-*unpacked && rm -rf dist/__snap* && rm -rf dist/win-*unpacked",
    "lint": "eslint --ext .tsx,.ts .",
    "download": "node ./scripts/download.js",
    "postinstall": "electron-builder install-app-deps && patch-package"
  },
  "dependencies": {
    "@sentry/electron": "^4.2.0",
    "@types/lodash": "^4.14.191",
    "chalk": "4.x",
    "chatgpt": "^5.0.5",
    "classnames": "^2.3.2",
    "clsx": "^1.1.1",
    "crawler": "^1.4.0",
    "electron-is-dev": "1.1.0",
    "electron-re": "^1.3.0",
    "electron-store": "^8.0.1",
    "electron-updater": "^5.3.0",
    "electron-window-state": "^5.0.3",
    "i18next": "^20.6.1",
    "ipv6": "*",
    "js-base64": "^2.4.0",
    "keyv": "^4.5.2",
    "keyv-file": "^0.2.0",
    "lodash": "^4.17.21",
    "node-abort-controller": "^3.1.1",
    "node-fetch": "2.6.0",
    "node-schedule": "^2.1.1",
    "puppeteer-core": "^19.6.2",
    "puppeteer-in-electron2": "^3.0.6",
    "qrcode": "^1.4.4",
    "quick-lru": "^6.1.1",
    "socks": "1.x",
    "tapable": "^2.2.1",
    "uuid": "7.0.2",
    "v8-compile-cache": "^2.3.0",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/crawler": "^1.2.2",
    "@types/i18next": "^13.0.0",
    "@types/js-base64": "^2.3.0",
    "@types/node": "^16.11.11",
    "@types/node-fetch": "2.5.5",
    "@types/node-schedule": "^2.1.0",
    "@types/qrcode": "^1.4.1",
    "@types/uuid": "7.0.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "compressing": "^1.5.1",
    "cross-env": "^7.0.3",
    "dotenv": "8.2.0",
    "electron": "18.0.3",
    "electron-builder": "^23.4.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-notarize": "0.3.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.30.1",
    "event-stream": "^4.0.1",
    "https-proxy-agent": "^5.0.1",
    "husky": "4.2.3",
    "jsqr": "^1.4.0",
    "lint-staged": "10.0.9",
    "nodemon": "2.0.2",
    "patch-package": "^6.4.7",
    "prettier": "2.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^3.17.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "targz": "1.0.1",
    "turbo": "^1.8.3",
    "typescript": "^4.5.2",
    "v8-inspect-profiler": "^0.1.0",
    "wait-on": "4.0.1",
    "yorkie": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run clean && lint-staged && npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=16.19.1",
    "npm": ">=8.19.3"
  },
  "browserslist": [
    "Electron >= 18"
  ],
  "__npminstall_done": false
}
